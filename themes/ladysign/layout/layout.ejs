<!DOCTYPE html>
<html class="no-js" lang="en" itemscope itemtype="https://schema.org/Blog">

<head>
  <meta charset="utf-8">
  <meta name="robots" content="all">
  <meta name="google-site-verification" content="jv8JHFCeqmxANKM3mXBtnW1KMtnYR2HJbTh60FEO9eE">
  <meta name="HandheldFriendly" content="True">
  <meta name="MobileOptimized" content="640">
  <link type="img/ico" rel="shortcut icon" href="/favicon.ico">
 <%
  var title = page.title;

  if (is_archive()){
    title = __('archive_a');

    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = __('category') + ': ' + page.category;
  } else if (is_tag()){
    title = __('tag') + ': ' + page.tag;
  }
  %>
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <title itemprop="name"><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
  <meta itemprop="creator" content="Lee Boonstra">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
 
  <%- open_graph({twitter_id: theme.twitter, google_plus: theme.google_plus, fb_admins: theme.fb_admins, fb_app_id: theme.fb_app_id}) %>
  <% if (theme.rss){ %>
    <link rel="alternate" href="<%= url_for(theme.rss) %>" title="<%= config.title %>" type="application/atom+xml">
  <% } %>

  <link rel="preload" href="/assets/styles/main.css'" as="style">

  <link rel="preload" href="/serviceworker.js" as="script">
  <link rel="preload" href="/assets/js/vendor/modernizr-3.6.0.min.js" as="script">
  <link rel="preload" href="/assets/js/vendor/jquery-3.3.1.min.js" as="script">
  <link rel="preload" href="/assets/js/vendor/popper.min.js" as="script">
  <link rel="preload" href="/assets/js/vendor/bootstrap.min.js" as="script">
  <link rel="preload" href="/assets/js/disqus-lazy-min.js" as="script">

  <link rel="preload" href="/assets/fonts/icomoon.eot" as="font">
  <link rel="preload" href="/assets/fonts/icomoon.ttf" as="font">
  <link rel="preload" href="/assets/fonts/icomoon.svg" as="font">
  <link rel="preload" href="/assets/fonts/icomoon.woff" as="font">
  <link rel="preload" href="/assets/fonts/consolas-webfont.eot" as="font">
  <link rel="preload" href="/assets/fonts/consolas-webfont.ttf" as="font">
  <link rel="preload" href="/assets/fonts/consolas-webfont.svg" as="font">
  <link rel="preload" href="/assets/fonts/consolas-webfont.woff" as="font">
  <link rel="preload" href="/assets/fonts/Mathlete-Skinny-webfont.eot" as="font">
  <link rel="preload" href="/assets/fonts/Mathlete-Skinny-webfont.ttf" as="font">
  <link rel="preload" href="/assets/fonts/Mathlete-Skinny-webfont.svg" as="font">
  <link rel="preload" href="/assets/fonts/Mathlete-Skinny-webfont.woff" as="font">
  <link rel="preload" href="/assets/fonts/oswald-regular-webfont.eot" as="font">
  <link rel="preload" href="/assets/fonts/oswald-regular-webfont.ttf" as="font">
  <link rel="preload" href="/assets/fonts/oswald-regular-webfont.svg" as="font">
  <link rel="preload" href="/assets/fonts/oswald-regular-webfont.woff" as="font">
  <link rel="preload" href="/assets/fonts/Roboto-Regular-webfont.eot" as="font">
  <link rel="preload" href="/assets/fonts/Roboto-Regular-webfont.ttf" as="font">
  <link rel="preload" href="/assets/fonts/Roboto-Regular-webfont.svg" as="font">
  <link rel="preload" href="/assets/fonts/Roboto-Regular-webfont.woff" as="font">

  <link rel="manifest" href="/manifest.json">
</head>

<body>
  <!--[if lte IE 9]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
  <![endif]-->
          
<div class="wrapper">
  <header>
  <div class="filler"></div>
  <nav class="navbar navbar-expand-lg navbar-light navbar-inverse navbar-fixed-top">
      <button class="navbar-toggler btn btn-dark" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
        <span class="iconmenu"></span>
      </button>
      <a class="navbar-brand" href="/" title="<%-config.title %>"><%- config.title %></a>
        
      <div class="collapse navbar-collapse" id="navbarTogglerDemo">
        
        <ul class="nav navbar-nav">
            <% for (var i in config.menu){ %>
              <li><a class="icon<%= i %> <%= is_current(theme.menu[i]) ? 'active' : '' %>"
                     href="<%- url_for(config.menu[i]) %>"><%= i %></a></li>
            <% } %>

            <li>
                <%- partial('_widget/category') %>
            </li>
        </ul>

  


        <form action="//google.com/search" method="get" accept-charset="UTF-8" class="search-form">
          <input type="search" name="q" class="search-form-input" placeholder="Search...">
          <button type="submit" class="btn btn-primary search-form-submit"><span class="iconsearch"></span></button>
          <input type="hidden" name="sitesearch" value="https://www.leeboonstra.com">
        </form>

    </nav>

    <% if (url_for(page.path) == '/index.html'){ %> 
      <div class="container-fluid">
          <div class="row">
            <div class="jumbotron">
                <%- config.subtitle %>

                <ul class="social">
                    <li><a href="http://github.com/savelee/" target="_blank" class="icon-github"><span>Github</span></a></li>
                    <li><a href="http://www.twitter.com/ladysign" target="_blank" class="icon-twitter"><span>Twitter</span></a></li>
                    <li><a href="http://nl.linkedin.com/in/leeboonstra/" target="_blank" class="icon-linkedin"><span>LinkedIn</span></a></li>
                    <% if (config.rss){ %>
                      <li><a id="nav-rss-link" class="icon-rss" href="<%- url_for(theme.rss) %>" title="<%= __('rss_feed') %>"></a></li>
                    <% } %>
                
                </ul>
            </div>
          </div>
        </div>

        <aside id="sidebarfront" class="sidebar">
            <%- partial('_partial/sidebar', null, {cache: !config.relative_link}) %>
        </aside>
      
    <% } %>

  </header>



  <main class="container-fluid content">
      <%- body %>
  </main>
  <footer class="foot">
    <div class="container">
      <div class="row">
        <div class="col col-sm-4">
            <%- partial('_widget/archive') %>
        </div>
        <div class="col col-sm-4">

          <h4>External Sites</h4>
            <ul>
                <li><a href="https://gist.github.com/savelee">My Gists Code Snippets</a></li>
                <li><a href="http://www.github.com/savelee">My Github</a></li>
                <li><a href="http://speakerdeck.com/savelee/">My Slidedecks</a></li>
                <li><a href="http://www.amazon.com/Lee-Boonstra/e/B00MRXE3Q0/ref=sr_ntt_srch_lnk_1?qid=1436781780&amp;sr=8-1">My Book</a></li>
                <li><a href="http://twitter.com/ladysign">My Twitter</a></li>
                <li><a href="https://www.linkedin.com/in/leeboonstra">
                My Linkedin</a></li>
            </ul>

        </div> 
        <div class="col col-sm-4">
            <%- partial('_widget/recent_posts') %>
        </div>
      </div>
    </div>
  </footer>
  <footer class="bottom">
    <div class="row">
      &copy; <%= date(new Date(), 'YYYY') %> <%= config.author || config.title %>
    </div>
  </footer>
</div>
  <%- partial('_partial/scripts') %>
  <%- partial('_partial/google-analytics') %>
  
  </body>
</html>
